                                    </Pane>
                                </children>
                            </Pane>
                            <Pane fx:id="officeAppointmentSelectorPane" layoutX="11.0" layoutY="164.0" prefHeight="119.0" prefWidth="239.0">
                                <children>
                                    <ChoiceBox fx:id="chosenTimeslot" layoutY="21.0" prefWidth="150.0"/>
                                    <ChoiceBox fx:id="chosenProvider" layoutY="61.0" prefWidth="150.0"/>
                                </children>
