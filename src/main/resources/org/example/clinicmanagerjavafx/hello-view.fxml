                                                <RadioButton fx:id="officeRadioButton" layoutX="11.0" layoutY="39.0" mnemonicParsing="false" onAction="#showOfficeDetailSelector" text="Office" />
                                                <RadioButton fx:id="imagingRadioButton" layoutX="76.0" layoutY="39.0" mnemonicParsing="false" onAction="#showImagingDetailSelector" prefHeight="13.0" prefWidth="67.0" text="Imaging" />
                                            </children>
                                        </Pane>
                                    </children>
                                </Pane>
                        <StackPane layoutY="163.0" prefHeight="89.0" prefWidth="181.0">
                           <children>
                              <Pane fx:id="imagingAppointmentSelectorPane" prefHeight="120.0" prefWidth="200.0" visible="false">
                                 <children>
                                    <ChoiceBox fx:id="chosenTimeslotImaging" layoutX="16.0" layoutY="14.0" prefWidth="150.0" />
                                    <ChoiceBox fx:id="chosenProviderImaging" layoutX="16.0" layoutY="49.0" prefWidth="150.0" />
                                 </children>
                              </Pane>
                              <Pane fx:id="officeAppointmentSelectorPane" prefHeight="57.0" prefWidth="144.0">
                                 <children>
                                    <ChoiceBox fx:id="chosenTimeslotOffice" layoutX="14.0" layoutY="14.0" prefWidth="150.0" />
                                    <ChoiceBox fx:id="chosenProviderOffice" layoutX="14.0" layoutY="50.0" prefWidth="150.0" />
                                 </children>
                              </Pane>
                           </children>
                        </StackPane>
                            </children>
