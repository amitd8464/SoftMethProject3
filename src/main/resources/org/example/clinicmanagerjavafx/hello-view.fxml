                                    <DatePicker fx:id="appointmentDatePicker" layoutX="14.0" layoutY="56.0" promptText="date of appointment"/>
                                    <Pane fx:id="visitTypePane" layoutY="97.0" prefHeight="96.0" prefWidth="239.0">
                                        <children>
                                            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#2197ff12" height="26.0" layoutX="14.0" layoutY="1.0" stroke="#00000047" strokeType="INSIDE" width="71.0"/>
