<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="org.example.clinicmanagerjavafx.HelloController">
    <children>
        <TabPane fx:id="tabPane" prefHeight="393.0" prefWidth="499.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="scheduleTab" text="Schedule">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <Text layoutX="299.0" layoutY="185.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Patient Info" />
                                <TextField layoutX="299.0" layoutY="217.0" promptText="first name" />
                                <TextField layoutX="299.0" layoutY="250.0" promptText="last name" />
                                <DatePicker layoutX="299.0" layoutY="287.0" promptText="date of birth" />
                                <Pane fx:id="appointmentInfoPane" layoutY="3.0" prefHeight="194.0" prefWidth="222.0">
                                    <children>
                                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#45a8ff5e" height="26.0" layoutX="14.0" layoutY="18.0" stroke="BLACK" strokeType="INSIDE" width="154.0" />
                                        <Text layoutX="23.0" layoutY="36.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Appointment Information" />
                                        <DatePicker fx:id="appointmentDatePicker" layoutX="14.0" layoutY="56.0" promptText="date of appointment" />
                                        <Pane fx:id="visitTypePane" layoutY="97.0" prefHeight="96.0" prefWidth="239.0">
                                            <children>
                                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#2197ff12" height="26.0" layoutX="14.0" layoutY="1.0" stroke="#00000047" strokeType="INSIDE" width="71.0" />
